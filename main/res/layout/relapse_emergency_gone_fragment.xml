<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.act4heart.act4heart.RelapseEmergencyGoneFragment">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Det är viktigt att du är säker på att du inte har några kvarstående besvär. "
        android:id="@+id/textView7"
        android:layout_below="@+id/textView3"
        android:layout_centerHorizontal="true"
        android:textSize="17dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        android:layout_marginRight="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Är du säker på detta så är det fortfarande viktigt att du är uppmärksam kring hur du mår och vad du känner. Du bör även berätta för en närstående eller kollega att du upplevt besvär."
        android:id="@+id/textView14"
        android:textSize="17dp"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        android:layout_marginRight="30dp" />
</RelativeLayout>
